platform :ios, '10.0'

use_frameworks!

def shared_pods
  	pod 'WebLinking', :git => 'https://github.com/akkyie/WebLinking.swift.git'
  	pod 'Kingfisher', '~> 4.0'
end

target 'Technical Challenge' do
 	shared_pods
end

target 'Technical ChallengeTests' do
    shared_pods
  end

  target 'Technical ChallengeUITests' do
    shared_pods
  end
